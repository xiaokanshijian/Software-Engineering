                                        {{ item.meta.name }}\r\n                                </router-link>\r\n                                <div :class=\"[activePath == item.path ? 'top-left' : '', 'corner']\"></div>\r\n                                <div :class=\"[activePath == item.path ? 'top-right' : '', 'corner']\"></div>\r\n                                <div :class=\"[activePath == item.path ? 'bottom-left' : '', 'corner']\"></div>\r\n                                <div :class=\"[activePath == item.path ? 'bottom-right' : '', 'corner']\"></div>\r\n                        </div>\r\n\r\n                </div>\r\n                <div id=\"header-time\">\r\n                        {{ currentDateTime }}\r\n                </div>\r\n        </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n        data() {\r\n                return {\r\n                        activePath: \"/\",\r\n                        routerLink: [],\r\n                        currentDateTime: this.getCurrentDateTime()\r\n                }\r\n        },\r\n        created() {\r\n                this.routerLink = this.$router.options.routes;\r\n                console.log(this.routerLink)\r\n                this.activePath = this.$route.path\r\n                this.interval = setInterval(() => {\r\n                        this.currentDateTime = this.getCurrentDateTime();\r\n                }, 1000);\r\n                let token = sessionStorage.getItem('Authorization');\r\n                if (!(token === null || token === '')) {\r\n                        this.isLogin = true;\r\n                }\r\n        },\r\n        computed: {\r\n                isLogin() {\r\n                        return this.$store.state.isLogin;\r\n                }\r\n        },\r\n        methods: {\r\n                getCurrentDateTime() {\r\n                        const now = new Date();\r\n                        const year = now.getFullYear();\r\n                        const month = (now.getMonth() + 1).toString().padStart(2, '0');\r\n                        const day = now.getDate().toString().padStart(2, '0');\r\n                        const hours = now.getHours().toString().padStart(2, '0');\r\n                        const minutes = now.getMinutes().toString().padStart(2, '0');\r\n                        const seconds = now.getSeconds().toString().padStart(2, '0');\r\n                        return `${year}-${month}-${day} ${hours}:${minutes}:${seconds}`;\r\n                },\r\n                routerChange(path) {\r\n                        if (path === '/login') {\r\n                                // window.location.href = 'http://127.0.0.1:8000/xadmin';\r\n                                isLogin = false;\r\n                        } else {\r\n                        this.$router.push(path);\r\n    }\r\n                }\r\n        }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n#header-time {\r\n        color: #52a2e4;\r\n        font-size: 17px;\r\n}\r\n\r\n.cool-border {\r\n        position: relative;\r\n}\r\n\r\n.corner {\r\n        position: absolute;\r\n        width: 0;\r\n        height: 0;\r\n        border: 5px solid transparent;\r\n}\r\n\r\n.top-left {\r\n        top: 0;\r\n        left: 0;\r\n        border-top-color: #fff;\r\n        border-left-color: #fff;\r\n}\r\n\r\n.top-right {\r\n        top: 0;\r\n        right: 0;\r\n        border-top-color: #fff;\r\n        border-right-color: #fff;\r\n}\r\n\r\n.bottom-left {\r\n        bottom: 0;\r\n        left: 0;\r\n        border-bottom-color: #fff;\r\n        border-left-color: #fff;\r\n}\r\n\r\n.bottom-right {\r\n        bottom: 0;\r\n        right: 0;\r\n        border-bottom-color: #fff;\r\n        border-right-color: #fff;\r\n}\r\n\r\n.content {\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n}\r\n\r\n#header {\r\n        display: flex;\r\n        padding: 10px 150px 0 150px;\r\n        align-items: center;\r\n        justify-content: space-between;\r\n\r\n        #header-left {\r\n                justify-content: flex-start;\r\n                h1 {\r\n                        color: #fff;\r\n                        font-size: 22px;\r\n                }\r\n        }\r\n\r\n        #header-nav {\r\n                display: flex;\r\n\r\n                .header-nav-item {\r\n                        margin-right: 15px;\r\n                        padding: 10px 22px;\r\n                        border: 1px solid transparent;\r\n                        border-radius: 80px;\r\n                }\r\n\r\n                .header-nav-item.active {\r\n                        background: #1b2d4a;\r\n                }\r\n\r\n                .nav {\r\n                        text-decoration: none;\r\n                        color: #52a2e4;\r\n                        font-size: 15px;\r\n                }\r\n        }\r\n\r\n        // #header-time{\r\n        //     font-size: 17px;\r\n        // }\r\n}\r\n</style>"]}]}